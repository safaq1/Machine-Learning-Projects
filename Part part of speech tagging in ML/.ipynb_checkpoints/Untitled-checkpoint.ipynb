    "import nltk\n",
    "nltk.download('averaged_perception_tagger')\n",
    "from nltk import word_tokenize, pos_tag\n",
    "sentence = \"I will move to Skardu forever\"\n",
    "tokens  = word_tokenize(sentence)\n",
    "tags = pos_tag(tokens)\n",
    "print(tags)"
